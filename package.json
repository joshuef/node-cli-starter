{
  "name": "s-sync",
  "version": "1.0.0",
  "main": "upload.js",
  "license": "MIT",
  "bin": "lib/upload.js",
  "scripts": {
    "bash-test": "for d in $(seq 1 5); do yarn dry-run -s dist || break; sleep 1; done;",
    "build": "rimraf ./lib && babel src -d lib",
    "dry-run": "NODE_ENV=test babel-node src/upload",
    "dry-run-built": "NODE_ENV=test node ./lib -s dist",
    "get": "NODE_ENV=test babel-node src/get",
    "lint": "eslint --cache --format=node_modules/eslint-formatter-pretty .",
    "lint-fix": "yarn run lint --fix",
    "prepush": "yarn lint && yarn test",
    "start": "babel-node src/upload.js",
    "test": "jest"
  },
  "dependencies": {
    "@maidsafe/safe-node-app": "git+https://github.com/bochaco/safe_app_nodejs#ee93103cbf2839f66d170def6a16df26954d375c",
    "bristol": "0.4.0",
    "commander": "2.17.1",
    "fs-extra": "7.0.0",
    "jsonld": "1.0.4",
    "klaw": "3.0.0",
    "mime-types": "2.1.20",
    "palin": "2.2.0",
    "rdf": "4.0.1",
    "rdflib": "0.17.1",
    "request": "2.88.0",
    "request-promise-native": "1.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.4.2",
    "eslint": "5.5.0",
    "eslint-formatter-pretty": "1.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jest": "21.22.0",
    "eslint-plugin-promise": "4.0.0",
    "jest": "23.5.0",
    "rimraf": "2.6.2"
  }
}
