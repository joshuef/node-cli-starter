{
  "name": "s-sync",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "babel": {
    "presets": [
      "env"
    ]
  },
  "scripts": {
      "build": "yarn babel src --out-dir lib",
      "start": "yarn run babel-node src/index.js",
      "dry-run": "NODE_ENV=test yarn run babel-node src/index.js",
      "dry-run-built": "NODE_ENV=test node ./lib -s dist",
	  "bash-test": "for d in $(seq 1 10); do yarn dry-run -s dist || break; sleep 5; done;"
  },
  "dependencies": {
    "babel": "6.23.0",
    "babel-cli": "6.26.0",
    "babel-preset-env": "1.7.0",
    "commander": "2.17.1",
    "fs-extra": "7.0.0",
    "jsonld": "1.0.4",
    "klaw": "3.0.0",
    "mime-types": "2.1.19",
    "rdf": "4.0.1",
    "rdflib": "0.17.0",
    "request": "2.88.0",
    "request-promise-native": "1.0.5",
    "winston": "3.0.0"
  },
  "devDependencies": {
    "rimraf": "2.6.2"
  }
}
